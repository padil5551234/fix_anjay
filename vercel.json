{
  "version": 2,
  "framework": null,
  "outputDirectory": "public",
  "builds": [
    {
      "src": "api/index.php",
      "use": "vercel-php@0.7.3",
      "config": {
        "buildCommand": "bash build.sh",
        "includeFiles": [
          "build.sh",
          "vite.config.js",
          "postcss.config.js",
          "tailwind.config.js",
          "package.json",
          "package-lock.json",
          "resources/css/**",
          "resources/js/**",
          "app/**",
          "bootstrap/**",
          "config/**",
          "routes/**",
          "resources/views/**",
          "storage/framework/**",
          "storage/app/public/**",
          "public/build/**",
          "public/css/**",
          "public/js/**",
          "vendor/**"
        ],
        "excludeFiles": [
          "node_modules/**",
          "tests/**",
          "storage/logs/**",
          "storage/framework/cache/**",
          "storage/framework/sessions/**",
          "storage/framework/views/**",
          ".git/**",
          "*.md",
          "*.log",
          "composer.json",
          "composer.lock",
          "phpunit.xml",
          ".env.example",
          "database/migrations/**",
          "database/seeders/**",
          "database/factories/**",
          "public/adminLTE/plugins/datatables-autofill/**",
          "public/adminLTE/plugins/codemirror/**",
          "public/adminLTE/plugins/inputmask/**",
          "public/adminLTE/plugins/summernote/**",
          "public/adminLTE/plugins/tempusdominus-bootstrap-4/**",
          "public/adminLTE/plugins/ion-rangeslider/**",
          "public/adminLTE/plugins/bootstrap-slider/**",
          "public/adminLTE/plugins/jquery-mapael/**",
          "public/adminLTE/plugins/sparklines/**",
          "public/adminLTE/plugins/select2-bootstrap4-theme/**",
          "public/adminLTE/plugins/toastr/**",
          "public/adminLTE/plugins/jquery-mousewheel/**",
          "public/adminLTE/plugins/jquery-ui/**",
          "public/adminLTE/build/**",
          "public/adminLTE/pages/**",
          "vendor/laravel/sail/**",
          "vendor/phpunit/**",
          "vendor/mockery/**",
          "vendor/fakerphp/**",
          "vendor/nunomaduro/collision/**",
          "vendor/spatie/laravel-ignition/**",
          "vendor/barryvdh/laravel-debugbar/**",
          "vendor/laravel/pint/**",
          "vendor/psy/psysh/**",
          "vendor/filp/whoops/**",
          "vendor/**/tests/**",
          "vendor/**/test/**",
          "vendor/**/Tests/**",
          "vendor/**/Test/**",
          "vendor/**/*.md",
          "vendor/**/.git/**",
          "vendor/**/docs/**",
          "vendor/**/documentation/**",
          "vendor/**/examples/**",
          "vendor/**/composer.json",
          "vendor/**/composer.lock",
          "vendor/**/phpunit.xml",
          "vendor/**/phpunit.xml.dist",
          "vendor/**/.gitignore",
          "vendor/**/.editorconfig",
          "vendor/**/.php_cs",
          "vendor/**/.php_cs.dist",
          "vendor/**/.travis.yml",
          "vendor/**/phpcs.xml",
          "vendor/**/phpcs.xml.dist",
          "vendor/**/LICENSE",
          "vendor/**/LICENSE.md",
          "vendor/**/CHANGELOG.md",
          "vendor/**/README.md"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/(css|js|images|fonts|icon|img|adminLTE|storage)/(.*)",
      "dest": "/$1/$2"
    },
    {
      "src": "/(.*\\.(css|js|json|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|webp|avif))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false"
  }
}