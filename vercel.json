{
  "version": 2,
  "framework": null,
  "buildCommand": "npm install && npm run build",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    },
    {
      "src": "api/index.php",
      "use": "vercel-php@0.7.3",
      "config": {
        "includeFiles": [
          "app/**",
          "bootstrap/**",
          "config/**",
          "routes/**",
          "resources/views/**",
          "storage/framework/**",
          "storage/app/public/**",
          "public/build/**",
          "public/css/**",
          "public/js/**",
          "vendor/**"
        ],
        "excludeFiles": [
          "node_modules/**",
          "tests/**",
          "storage/logs/**",
          "storage/framework/cache/**",
          "storage/framework/sessions/**",
          "storage/framework/views/**",
          ".git/**",
          "*.md",
          "*.log",
          "composer.json",
          "composer.lock",
          "package.json",
          "package-lock.json",
          "phpunit.xml",
          ".env.example",
          "database/migrations/**",
          "database/seeders/**",
          "public/adminLTE/plugins/datatables-autofill/**",
          "public/adminLTE/plugins/codemirror/**",
          "public/adminLTE/plugins/inputmask/**",
          "vendor/laravel/sail/**",
          "vendor/phpunit/**",
          "vendor/mockery/**",
          "vendor/fakerphp/**",
          "vendor/nunomaduro/collision/**",
          "vendor/spatie/laravel-ignition/**",
          "vendor/barryvdh/laravel-debugbar/**",
          "vendor/laravel/pint/**",
          "vendor/**/tests/**",
          "vendor/**/test/**",
          "vendor/**/Tests/**",
          "vendor/**/Test/**",
          "vendor/**/*.md",
          "vendor/**/.git/**",
          "vendor/**/docs/**",
          "vendor/**/documentation/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/(css|js|images|fonts|icon|img|adminLTE|storage)/(.*)",
      "dest": "/$1/$2"
    },
    {
      "src": "/(.*\\.(css|js|json|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|webp|avif))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false"
  }
}